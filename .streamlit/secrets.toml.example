
# Environment Configuration
[environment]
type = "development"  # Can be "development", "testing", or "production"
debug = true
logging_level = "DEBUG"

# Database Configuration
[database]
host = "your_host"
port = 5432
database = "your_db"
username = "your_user"
password = "your_password"

# Authentication Configuration (if needed)
[auth]
enable_login = false
secret_key = "your_secret_key"

# Feature Flags
[features]
enable_experimental = true
show_developer_tools = true
cache_ttl = 3600

# Logging Configuration
[logging]
level = "INFO"
file_path = "logs/app.log"
enable_telemetry = false
